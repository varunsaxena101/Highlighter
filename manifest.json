{
    "name": "SearchStash",

    "key": "",

    "oauth2": {
        "client_id": "889952242965-dp4encv89d30ut5beer3hmbbi5cvkuom.apps.googleusercontent.com",
        "scopes":[
            "https://www.googleapis.com/auth/contacts.readonly",
            "profile",
            "https://www.googleapis.com/auth/plus.login"
        ]
    },

    "version": "1.0",

    "description": "Save some text from a webpage and look it up later",

    "permissions": ["storage", "declarativeContent", "activeTab", "tabs", "contextMenus", "identity"],

    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],

    "browser_action": {
    	"default_popup": "popup.html",
    	"default_icon": {
    		"16": "images/get_started16.png",
    		"32": "images/get_started32.png",
    		"48": "images/get_started48.png",
    		"128": "images/get_started128.png"
    	}
    },

    "icons": {
    	"16": "images/get_started16.png",
		"32": "images/get_started32.png",
		"48": "images/get_started48.png",
		"128": "images/get_started128.png"
    },

    "manifest_version": 2
 }